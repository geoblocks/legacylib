{
    "opts": {
        "recurse": true,
        "template": "../config/jsdoc/info"
    },
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": "\\.js$",
        "exclude": ["src/index.js"]
    },
    "plugins": [
        "../config/jsdoc/info/api-plugin"
    ]
}
